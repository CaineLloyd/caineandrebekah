{
  "database": {
    "rules": "database.rules.json"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"%RESOURCE_DIR%\" run lint"
    ]
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [ {
      "source": "/index.html",
      "destination": "https://www.caineandrebekah.com",
      "type" : 301
    }, {
      "source": "/index",
      "destination": "https://www.caineandrebekah.com",
      "type" : 301
    } ],
    "rewrites": [ {
      "source": "/example",
      "destination": "example"
    }, {
      "source": "/* //THIS WOULD REDIRECT ALL URLS",
      "function": "server"
    }, {
      "source": "/router",
      "function": "router"
    } ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}

{
  "database": {
    "rules": "database.rules.json"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"%RESOURCE_DIR%\" run lint"
    ]
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
      "redirects": [ {
    "source" : "/gallery",
    "destination" : "/gallery/gallery.html",
    "type" : 301
  } ],
      "rewrites": [ {
    "source": "/blog",
    "destination": "/blog/blog.html"
  } ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
